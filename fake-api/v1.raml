                                            "requirement": {
                                                "default": "ANY",
                                                "required": false,
                                                "type": [
                                                    "string",
                                                    "number"
                                                ],
                                                "description": "determines if the counter requires all conditions or only one; 0:ALL, 1:ANY",
                                                "enum": [
                                                    0,
                                                    "ALL",
                                                    1,
                                                    "ANY"
                                                ]
                                            },
                                            "createdAt": {
                                                "type": "number",
                                                "description": "the time of creating the counter in milliseconds"
                                            },
                                            "name": {
                                                "description": "the name of the counter, can be used in conditions on the left side",
                                                "minLength": 1,
                                                "required": true,
                                                "maxLength": 50,
                                                "pattern": "^[a-zA-Z0-9\\_]+$",
                                                "type": "string"
                                            },
                                            "eventSettable": {
                                                "default": false,
                                                "required": false,
                                                "type": "boolean",
                                                "description": "determines if the counter can be set by an event"
                                            },
                                            "value": {
                                                "default": 0,
                                                "required": false,
                                                "type": "number",
                                                "description": "the global value of the counter"
                                            },
                                            "globalCounter": {
                                                "default": false,
                                                "required": false,
                                                "type": "boolean",
